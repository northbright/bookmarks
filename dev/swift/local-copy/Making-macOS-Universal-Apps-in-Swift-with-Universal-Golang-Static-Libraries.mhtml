                  </span>
                  <!-- end .post-date -->
                 =20
                  <span class=3D"by-author"> by                    <span cl=
ass=3D"author vcard">
                      <a class=3D"url fn n" rel=3D"author" title=3D"View al=
l posts by hrbrmstr" href=3D"http://rud.is/">hrbrmstr</a>
                    </span>
                    <!-- end .author -->
                  </span>
                  <!-- end .by-author -->
                 =20
                                   =20
